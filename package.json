  "license": "LGPL-3.0",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/connexta/kanri.git"
  },
  "engines": {
    "node": ">=0.10.5"
  },
  "devDependencies": {
    "@types/dropzone": "5.0.6",
    "@types/iframe-resizer": "3.5.7",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.throttle": "4.1.5",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.5",
    "@types/react-select": "3.0.2",
    "@types/socket.io": "2.1.3",
    "@types/socket.io-client": "1.4.32",
    "@types/styled-components": "4.0.3",
    "@connexta/ace": "git://github.com/andrewkfiedler/ace.git#e0267416f6bdc5f1de30dbf4f20729ad32f95e01",
    "@connexta/eslint-config-connexta": "git://github.com/connexta/eslint-config-connexta.git#3f0caece6210d25c16863d472026fbfa069521a6",
    "chokidar": "3.0.2",
    "cpx": "1.5.0",
    "express": "4.17.1",
    "lodash.debounce": "4.0.8",
    "rimraf": "3.0.0",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@connexta/atlas": "0.0.46",
    "dropzone": "5.5.1",
    "font-awesome": "4.7.0",
    "formik": "1.5.8",
    "iframe-resizer": "4.1.1",
    "lodash.throttle": "4.1.1",
    "@connexta/logviewer": "git://github.com/connexta/logviewer.git#17e7f603fbe5ba5d8edcd64c07787d065617deaa",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "styled-components": "4.3.2",
    "yup": "0.27.0"
  },
  "scripts": {
    "copy:package": "cpx package.json dist && cpx \"src/**/!(*.tsx)\" \"dist/src\"",
    "start": "ace start && yarn capture:mocks",
    "start:netlify": "ace start --env netlify",
    "prebuild": "rimraf dist && yarn copy:package",
    "build": "tsc",
    "build:standalone": "ace bundle --env netlify && cpx dev/netlify.toml target/webapp/",
    "publish:npm": "yarn build && npm publish dist",
    "m2": "yarn install:m2",
    "capture:mocks": "ts-node -P ./tsconfig.json ./dev/capture-mocks.tsx",
    "watch": "ts-node -P ./tsconfig.json ./dev/watch.tsx"
  },
  "ace": {
    "whitelist": [],
    "resolve": [
      "react",
      "react-dom",
      "@material-ui/styles",
      "styled-components"
    ]
  },
  "files": [
    "src/main/webapp",
    "target/webapp",
    "src/main/resources"
  ],
  "main": "src/main/webapp/main.js",
  "resolutions": {
    "core-js": "2.6.9",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4"
  }
}
